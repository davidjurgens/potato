{
  "id": "rse",
  "name": "Rosenberg Self-Esteem Scale",
  "short_name": "RSE",
  "description": "A 10-item scale that measures global self-worth by measuring both positive and negative feelings about the self. It is the most widely used self-esteem measure in social science research.",
  "url": "https://socy.umd.edu/about-us/rosenberg-self-esteem-scale",
  "reference": "Rosenberg, M. (1965). Society and the adolescent self-image. Princeton, NJ: Princeton University Press.",
  "reference_url": "https://doi.org/10.1515/9781400876136",
  "items_count": 10,
  "domains": ["self_concept", "self_esteem"],
  "scoring": {
    "method": "sum",
    "reverse_items": [2, 5, 6, 8, 9],
    "range": [0, 30],
    "interpretation": {
      "0-15": "Low self-esteem",
      "15-25": "Normal self-esteem",
      "25-30": "High self-esteem"
    }
  },
