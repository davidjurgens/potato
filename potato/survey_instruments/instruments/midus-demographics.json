{
  "id": "midus-demographics",
  "name": "MIDUS Core Demographics",
  "short_name": "MIDUS-Demo",
  "description": "Core demographic questions from the Midlife in the United States Study (MIDUS), a national longitudinal study of health and well-being of Americans from young adulthood to old age.",
  "url": "https://midus.wisc.edu",
  "reference": "Brim, O. G., Ryff, C. D., & Kessler, R. C. (2004). How Healthy Are We?: A National Study of Well-Being at Midlife. University of Chicago Press.",
  "reference_url": "https://doi.org/10.7910/DVN/OSCXQH",
  "items_count": 13,
  "domains": ["demographics"],
  "instructions": "Please answer the following questions about yourself.",
  "questions": [
    {
      "name": "midus_age",
      "description": "What is your age?",
      "annotation_type": "textbox",
      "label_requirement": {"required": true}
    },
    {
      "name": "midus_sex",
      "description": "What is your sex?",
      "annotation_type": "radio",
      "labels": [
        {"name": "male", "label": "Male"},
        {"name": "female", "label": "Female"}
      ],
      "label_requirement": {"required": true}
    },
    {
      "name": "midus_race",
      "description": "Which of the following best describes your racial background?",
      "annotation_type": "radio",
      "labels": [
        {"name": "white", "label": "White"},
        {"name": "black", "label": "Black and/or African American"},
        {"name": "native", "label": "Native American or Alaskan Native"},
        {"name": "asian", "label": "Asian"},
        {"name": "pacific", "label": "Native Hawaiian or Pacific Islander"},
        {"name": "other", "label": "Other"}
      ],
      "label_requirement": {"required": true}
    },
    {
      "name": "midus_hispanic",
      "description": "Are you of Hispanic, Latino, or Spanish origin?",
      "annotation_type": "radio",
      "labels": [
        {"name": "yes", "label": "Yes"},
        {"name": "no", "label": "No"}
      ],
      "label_requirement": {"required": true}
    },
    {
      "name": "midus_education",
      "description": "What is the highest grade of school or year of college you completed?",
      "annotation_type": "radio",
      "labels": [
        {"name": "no_school", "label": "No school/some grade school"},
        {"name": "8th_grade", "label": "Eighth grade/junior high school"},
        {"name": "some_hs", "label": "Some high school"},
        {"name": "ged", "label": "GED"},
        {"name": "hs_grad", "label": "High school graduate"},
        {"name": "some_college", "label": "Some college (no degree)"},
        {"name": "associates", "label": "Associate's degree (AA)"},
        {"name": "bachelors", "label": "Bachelor's degree (BA, BS)"},
        {"name": "some_graduate", "label": "Some graduate school"},
        {"name": "masters", "label": "Master's degree (MA, MS, MBA)"},
        {"name": "professional", "label": "Professional degree (MD, JD, DDS)"},
        {"name": "doctorate", "label": "Doctoral degree (PhD, EdD)"}
      ],
      "label_requirement": {"required": true}
    },
    {
      "name": "midus_marital",
      "description": "What is your current marital status?",
      "annotation_type": "radio",
      "labels": [
        {"name": "married", "label": "Married"},
        {"name": "separated", "label": "Separated"},
        {"name": "divorced", "label": "Divorced"},
        {"name": "widowed", "label": "Widowed"},
        {"name": "never_married", "label": "Never married"}
      ],
      "label_requirement": {"required": true}
    },
    {
      "name": "midus_living_with_partner",
      "description": "Are you currently living with someone in a marriage-like relationship?",
      "annotation_type": "radio",
      "labels": [
        {"name": "yes", "label": "Yes"},
        {"name": "no", "label": "No"}
      ],
      "label_requirement": {"required": true}
    },
    {
      "name": "midus_children",
      "description": "How many children have you ever had (including any who have passed away)?",
      "annotation_type": "radio",
      "labels": [
        {"name": "0", "label": "None"},
        {"name": "1", "label": "1"},
        {"name": "2", "label": "2"},
        {"name": "3", "label": "3"},
        {"name": "4", "label": "4"},
        {"name": "5plus", "label": "5 or more"}
      ],
      "label_requirement": {"required": true}
    },
    {
      "name": "midus_employment",
      "description": "Which of the following best describes your current employment situation?",
      "annotation_type": "radio",
      "labels": [
        {"name": "full_time", "label": "Working full-time"},
        {"name": "part_time", "label": "Working part-time"},
        {"name": "self_employed", "label": "Self-employed"},
        {"name": "unemployed_looking", "label": "Unemployed and looking for work"},
        {"name": "temporarily_laid_off", "label": "Temporarily laid off"},
        {"name": "retired", "label": "Retired"},
        {"name": "homemaker", "label": "Full-time homemaker"},
        {"name": "student", "label": "Full-time student"},
        {"name": "disabled", "label": "Permanently disabled"},
        {"name": "other", "label": "Other"}
      ],
      "label_requirement": {"required": true}
    },
    {
      "name": "midus_income",
      "description": "What is your total personal income before taxes in the past 12 months?",
      "annotation_type": "radio",
      "labels": [
        {"name": "under_10k", "label": "Less than $10,000"},
        {"name": "10k_20k", "label": "$10,000 - $19,999"},
        {"name": "20k_35k", "label": "$20,000 - $34,999"},
        {"name": "35k_50k", "label": "$35,000 - $49,999"},
        {"name": "50k_75k", "label": "$50,000 - $74,999"},
        {"name": "75k_100k", "label": "$75,000 - $99,999"},
        {"name": "100k_150k", "label": "$100,000 - $149,999"},
        {"name": "150k_plus", "label": "$150,000 or more"}
      ],
      "label_requirement": {"required": true}
    },
    {
      "name": "midus_financial_situation",
      "description": "How would you rate your current financial situation?",
      "annotation_type": "slider",
      "min": 0,
      "max": 10,
      "min_label": "Worst possible",
      "max_label": "Best possible",
      "label_requirement": {"required": true}
    },
    {
      "name": "midus_religion",
      "description": "What is your religious preference?",
      "annotation_type": "radio",
      "labels": [
        {"name": "protestant", "label": "Protestant"},
        {"name": "catholic", "label": "Catholic"},
        {"name": "jewish", "label": "Jewish"},
        {"name": "other", "label": "Other religion"},
        {"name": "none", "label": "No religious preference"}
      ],
      "label_requirement": {"required": true}
    },
    {
      "name": "midus_born_us",
      "description": "Were you born in the United States?",
      "annotation_type": "radio",
      "labels": [
        {"name": "yes", "label": "Yes"},
        {"name": "no", "label": "No"}
      ],
      "label_requirement": {"required": true}
    }
  ]
}
